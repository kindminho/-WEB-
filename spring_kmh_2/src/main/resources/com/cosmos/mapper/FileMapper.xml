<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.cosmos.mapper.FileMapper">

  <select id="getFile" resultType="com.cosmos.domain.FileVo">
<!--   부등호(=, >, <)를 사용하기위해   -->
  <![CDATA[   	
    SELECT *
    FROM fileup
    WHERE board_no = #{bno}
  ]]>  
  </select>
  
  
  
  
</mapper>








